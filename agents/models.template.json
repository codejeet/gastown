{
  "providers": {
    "openai": {
      "baseUrl": "{{PROXY_URL}}",
      "api": "openai-completions",
      "models": [
        {
          "id": "{{MODEL_ID}}",
          "name": "{{MODEL_NAME}}",
          "reasoning": true,
          "input": ["text"],
          "cost": {
            "input": 0,
            "output": 0,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        }
      ],
      "apiKey": "{{API_KEY_ENV}}"
    }
  }
}
